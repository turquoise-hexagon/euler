(define (make-factorial n)
  (let ((acc (make-vector (+ n 1) 0)))
    (vector-set! acc 0 1)
    (do ((i 1 (+ i 1))) ((> i n))
      (vector-set! acc i (* (vector-ref acc (- i 1)) i)))
    (define (factorial n)
      (vector-ref acc n))
    factorial))

(define (make-binomial n)
  (let ((factorial (make-factorial n)))
    (define (binomial n k)
      (/ (factorial n) (* (factorial k) (factorial (- n k)))))
    binomial))

(define (solve a b)
  (let ((binomial (make-binomial a)))
    (do ((n 1 (+ n 1))
         (acc 0 (+ acc
                   (do ((k 1 (+ k 1))
                        (acc 0 (if (> (binomial n k) b)
                                 (+ acc 1)
                                 acc)))
                      ((> k n) acc)))))
      ((> n a) acc))))

(let ((_ (solve 100 #e1e6)))
  (print _) (assert (= _ 4075)))
